<?xml version="1.0" encoding="UTF-8"?>

<module rename-to="gwtcordovashowcase">

  <inherits name='com.google.gwt.user.User'/>
  <inherits name="com.florianf.gwtcordova.GwtCordova"/>
  <inherits name="com.florianf.gwtcordova.Camera"/>
  <inherits name="com.florianf.gwtcordova.Device"/>

  <entry-point class='com.florianf.gwtcordovashowcase.client.GwtCordovaShowcase'/>

  <source path='client'/>

  <add-linker name="xsiframe"/>

  <set-property name="user.agent" value="safari"/>
  <collapse-all-properties value="true" />

